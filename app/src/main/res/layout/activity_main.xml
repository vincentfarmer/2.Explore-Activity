<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.MainActivity">

    <Button
        android:id="@+id/to_second_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/to_second_activity"/>

    <Button
        android:id="@+id/to_baidu_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/to_baidu_activity"/>

    <Button
        android:id="@+id/to_dial_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/to_dial_activity"/>
</LinearLayout>


